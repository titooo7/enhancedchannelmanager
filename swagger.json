      tags:
      - api
    put:
      operationId: api_accounts_groups_update
      description: Update a group
      parameters:
      - name: data
        in: body
        required: true
        schema:
          $ref: '#/definitions/Group'
      responses:
        '200':
          description: ''
          schema:
            $ref: '#/definitions/Group'
      tags:
      - api
    patch:
      operationId: api_accounts_groups_partial_update
      description: Handles CRUD operations for Groups
      parameters:
      - name: data
        in: body
        required: true
        schema:
          $ref: '#/definitions/Group'
      responses:
        '200':

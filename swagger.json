          description: ''
          schema:
            $ref: '#/definitions/Channel'
      tags:
      - api
    delete:
      operationId: api_channels_channels_delete
      description: ''
      parameters: []
      responses:
        '204':
          description: ''
      tags:
      - api
    parameters:
    - name: id
      in: path
      description: A unique integer value identifying this channel.
      required: true

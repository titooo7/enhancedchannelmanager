    parameters: []
  /api/channels/channels/set-tvg-ids-from-epg/:
    post:
      operationId: api_channels_channels_set_tvg_ids_from_epg
      description: Trigger a Celery task to set channel TVG-IDs from EPG data
      parameters:
      - name: data
        in: body
        required: true
        schema:
          $ref: '#/definitions/Channel'
